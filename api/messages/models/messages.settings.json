{
  "kind": "collectionType",
  "collectionName": "messages",
  "info": {
    "name": "messages"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "message": {
      "type": "richtext"
    },
    "contact_number": {
      "type": "string"
    },
    "createdAt": {
      "type": "datetime"
    },
    "updatedAt": {
      "type": "datetime"
    },
    "upload": {
      "via": "message",
      "model": "uploads"
    }
  }
}
